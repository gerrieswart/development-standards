{
	"parser": "babel-eslint",
	"plugins": [
		"react"
	],
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"globals": {
		"__DEV__": true,
		"__SERVER__": true
	},
	"ecmaFeatures": {
		"jsx": true
	},
	"rules": {
		"strict": [2, "never"],
		"indent": [2, "tab"],
		"quotes": [2, "single"]
	}
}
